{
  "enabled": true,
  "exclude_patterns": [
    ".dart_tool/**",
    "build/**",
    ".pub/**",
    "packages/**",
    "**/*.g.dart",
    "**/*.freezed.dart"
  ],
  "style_rules": {
    "enabled": true,
    "disabled_rules": []
